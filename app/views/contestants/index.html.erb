<h1> All Contestants </h1>

<% @contestants.each do |contestant| %>
  <section class="contestant-<%= contestant.id %>">
    <h2><%= contestant.name %></h2>
    <li>Projects: <% contestant.projects.each do |project| %>
                 <ul><%= project.name %></ul>
                 <% end %>
    </li>
  </section>
<% end %>
